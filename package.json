{
	"name": "nest-typescript-starter",
	"version": "1.0.0",
	"description": "Nest TypeScript starter repository",
	"license": "MIT",
	"prisma": {
		"seed": "ts-node --transpile-only --compiler-options {\"resolveJsonModule\":true} prisma/seed.ts"
	},
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "npx prisma generate && rm -R dist || nest build",
		"update": "npm run generate:typings && npx prisma db push",
		"generate:typings": "ts-node generate-typings.ts",
		"clean": "rm -R node_modules && rm -R yarn.lock && rm -R dist",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "npm run generate:typings && npx prisma db push && nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "echo 'No e2e tests implemented yet.'"
	},
	"dependencies": {
		"@apollo/server": "^4.9.3",
		"@nestjs/apollo": "^12.0.9",
		"@nestjs/common": "10.2.5",
		"@nestjs/core": "10.2.5",
		"@nestjs/graphql": "12.0.9",
		"@nestjs/platform-express": "10.2.5",
		"@prisma/client": "5.3.1",
		"@vendia/serverless-express": "^4.10.4",
		"axios": "^1.2.3",
		"cheerio": "^1.0.0-rc.12",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.0",
		"crypto-js": "^4.1.1",
		"graphql": "16.8.1",
		"graphql-subscriptions": "2.0.0",
		"node-telegram-bot-api": "^0.63.0",
		"reflect-metadata": "0.1.13",
		"rimraf": "5.0.1",
		"rxjs": "7.8.1"
	},
	"devDependencies": {
		"@hewmen/serverless-plugin-typescript": "^1.1.17",
		"@nestjs/cli": "10.1.17",
		"@nestjs/schematics": "10.0.2",
		"@nestjs/testing": "10.2.5",
		"@types/aws-lambda": "^8.10.121",
		"@types/crypto-js": "^4.1.1",
		"@types/express": "4.17.17",
		"@types/node": "20.6.2",
		"@types/node-telegram-bot-api": "^0.61.7",
		"@types/supertest": "2.0.12",
		"@typescript-eslint/eslint-plugin": "6.7.2",
		"@typescript-eslint/parser": "6.7.2",
		"eslint": "8.49.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "2.28.1",
		"jest": "29.7.0",
		"plugin": "^0.3.3",
		"prettier": "3.0.3",
		"prisma": "^5.3.1",
		"serverless-jetpack": "^0.11.2",
		"serverless-offline": "^12.0.4",
		"supertest": "6.3.3",
		"ts-jest": "29.1.1",
		"ts-loader": "9.4.4",
		"ts-morph": "19.0.0",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.2.0",
		"typescript": "^5.2.2"
	}
}